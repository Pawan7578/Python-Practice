   "source": [
    "# program for to find no. of characters and digits in a string\n",
    "x=input()\n",
    "alphabet=0\n",
    "digit=0\n",
    "for i in x:\n",
    "    if i.isdigit():\n",
    "        digit+=1\n",
    "    elif i.isalpha():\n",
    "        alphabet+=1\n",
    "print(\"No. of Digit:\",digit)\n",
    "print(\" No of Alphabet\",alphabet)"
   ]
  }
